<h1>Liste des membres du club</h1>

<div class="row">
  <div class="col-xs-8 col-xs-offset-2">
    <table class="table">
      <thead>
        <tr>
          <th>Nom</th>
          <th>Pr√©nom</th>
          <th>Email</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.last_name %></td>
            <td><%= user.first_name %></td>
            <td><%= user.email %></td>
            <td><%= link_to "Voir le profil", user_path(user), class: "btn btn-primary"%></td>
          </tr>
        <% end %>
      </tbody>
    </table
  </div>
</div>
